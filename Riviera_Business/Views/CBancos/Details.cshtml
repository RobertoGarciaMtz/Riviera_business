@model Riviera_Business.Models.CBanco

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del banco</h1>
<hr />
<div>
    
    <dl class="row">
        <dt class = "col-sm-3 bg-dark">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-9 bg-white">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>
</div>
<div class="text-right">
    <a class="btn btn-info mr-2" asp-action="Edit" asp-route-id="@Model.IdBanco">Edit</a> |
    <a class="btn btn-primary" asp-action="Index">Regresar</a>
</div>
