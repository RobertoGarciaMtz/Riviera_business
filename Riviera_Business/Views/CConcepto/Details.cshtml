@model Riviera_Business.Models.CConcepto

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del concepto</h1>
<hr />
<div>
    
    <dl class="row">
        <dt class = "col-sm-3 bg-dark">
            Cocepto
        </dt>
        <dd class = "col-sm-9 bg-white">
            @Html.DisplayFor(model => model.DescripcionConcepto)
        </dd>
    </dl>
</div>
<div class="text-right">
    <a class="btn btn-info" asp-action="Index">Regresar</a>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.IdCConcepto">Edit</a>
</div>
